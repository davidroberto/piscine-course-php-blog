{% include('partial/header.html.twig') %}

<main>

        <h1>Ajouter un article</h1>


        <form method="post" >

            <label>
                Titre
                <input type="text" name="title" />
            </label>

            <label>
                Contenu
                <input type="text" name="content" />
            </label>

            <input type="submit" />

        </form>


        {% if isRequestOk %}

            <p>L'article a bien été enregistré en BDD</p>

        {% endif %}

    </main>

